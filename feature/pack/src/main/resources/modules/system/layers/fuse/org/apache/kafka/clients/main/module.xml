<?xml version="1.0" encoding="UTF-8"?>
<module xmlns="urn:jboss:module:1.3" name="org.apache.kafka.clients">
  <properties>
    <property name="jboss.api" value="private" />
  </properties>
  <resources>
    <artifact name="${org.apache.kafka:kafka-clients}" />
    <artifact name="${net.jpountz.lz4:lz4}" />
    <artifact name="${org.xerial.snappy:snappy-java}" />
  </resources>
  <dependencies>
    <module name="javax.api" />
    <module name="org.osgi.core" />
    <module name="org.slf4j" />
  </dependencies>
  <exports>
    <exclude path="darwin/*" />
    <exclude path="linux/*" />
    <exclude path="win32/*" />
    <exclude path="net/jpountz**" />
    <exclude path="org/xerial**" />
  </exports>
</module>
