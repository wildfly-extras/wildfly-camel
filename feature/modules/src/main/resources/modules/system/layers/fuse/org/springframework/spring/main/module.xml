<!--
    This is referenced in the base layer from org.apache.cxf as an optional dependency
-->
<module xmlns="urn:jboss:module:1.1" name="org.springframework.spring">

    <dependencies>
        <module name="org.springframework.beans" export="true">
            <imports>
                <include path="META-INF" />
            </imports>
            <exports>
                <include path="META-INF" />
            </exports>
        </module>
        <module name="org.springframework.context" export="true">
            <imports>
                <include path="META-INF" />
            </imports>
            <exports>
                <include path="META-INF" />
            </exports>
        </module>
        <module name="org.springframework.core" export="true">
            <imports>
                <include path="META-INF" />
            </imports>
            <exports>
                <include path="META-INF" />
            </exports>
        </module>
    </dependencies>

</module>
